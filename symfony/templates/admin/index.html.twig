{% extends 'base.html.twig' %}

{% block title %}Tableau de bord administrateur{% endblock %}

{% block body %}
    <h1>Tableau de bord administrateur</h1>
    <h2>Promouvoir un utilisateur administrateur : </h2>
    {{ form_start(form)}}
    {{ form_errors(form) }}
    {{ form_row(form.email, {label: "E-mail"}) }}
    <button type="submit" class="btn btn-primary">Promouvoir</button>
    {{ form_end(form)}}
{% endblock %}
